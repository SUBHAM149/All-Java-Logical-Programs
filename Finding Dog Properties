//Finding Dog Properties
package com.sub.blc;

public class DogConstructor {
      private String name;
      private String color;
      private double height;
      private int age;
      
	  public DogConstructor(String name, String color, double height, int age) {
		super();
		this.name = name;
		this.color = color;
		this.height = height;
		this.age = age;
	  }
	  @Override
	  public String toString() {
		return "DogConstructor [name=" + name + ", color=" + color + ", height=" + height + ", age=" + age + "]";
	  }
      
}
package com.sub.elc;

import com.sub.blc.DogConstructor;

public class DogConstroctorDetails {

	public static void main(String[] args) {
		DogConstructor tommy= new DogConstructor("Tommy","white",2.5,3);
		System.out.println(tommy);

	}

}
