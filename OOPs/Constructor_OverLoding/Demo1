package constructor_overloding;

public class constructorOverlodingdemo1 {

	public static void main(String[] args) {
		new Calculator(12,12);

	}

}
class Calculator{
	public Calculator(int x,int y)
	{
		this(2.5,6.7);
		IO.println("Sum of two integer is: "+(x+y));
	}
	public Calculator(double x,double y)
	{
		this("java"," is a "," great language ");
		IO.println("Sum of two double is: "+(x+y));
	}
	public Calculator(String x,String y,String z)
	{
		super();
		IO.println("Concatenation of three String Is: "+(x+y+z));
	}
}
/* OUTPUT
Concatenation of three String Is: java is a  great language 
Sum of two double is: 9.2
Sum of two integer is: 24  */
