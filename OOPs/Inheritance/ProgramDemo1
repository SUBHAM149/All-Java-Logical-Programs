package com.sub.inheritance_understanding;
class Animal{
	public void eat()
	{
		IO.println("Animal is eating");
	}
}
class Dog extends Animal
{
	public void bark()
	{
		IO.println("Dog is Barking");
	}
}
public class ProgramDemo1 {
	public static void main(String[] args) {
		Dog d=new Dog();
		d.eat();
		d.bark();

	}

}
