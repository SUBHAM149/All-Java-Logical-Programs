package construct.sub.blc;

public class Student1 {
	    private String name;
	    private int marks;

	    
	    public Student1(String name, int marks) {
	        this.name = name;
	        this.marks = marks;
	    }

	    
	    public String getName() {
	        return name;
	    }
	    public void setName(String name) {
	        this.name = name;
	    }

	    public int getMarks() {
	        return marks;
	    }
	    public void setMarks(int marks) {
	        this.marks = marks;
	    }

	    
	    public String toString() {
	        return name;
	    }
	}
