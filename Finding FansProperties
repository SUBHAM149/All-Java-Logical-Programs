// Finding FansProperties
package com.sub.blc;

public class FansProperties {
    public String name;
    public   String coil;
    public int wings;
    
    public  void switchOn() 
    {
    	System.out.println("ON");
    }
    public void switchoff() {
    	System.out.println("OFF");
    }
}
package com.sub.elc;

import com.sub.blc.FansProperties;

public class Fanbehviour {

	public static void main(String[] args) {
		FansProperties fan=new FansProperties();
	    fan.name="Acc";
	    fan.coil="saco";
	    fan.wings=4;
	    fan.switchoff();
	    fan.switchOn();

	}

}
