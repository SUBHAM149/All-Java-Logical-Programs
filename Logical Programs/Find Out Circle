//Find Out Circle
package com.sub.blc;

public class Circle {
     public static String getAreaOfCircle(double radius)
     {
    	 if(radius<=0)
    			 {
    		 return ""+(-1);
    			 }
    	 else {
    		 final double PI=3.14;
    		 double area =PI*radius*radius;
    		 return ""+area;
    	 }
}
}
package com.sub.elc;

import com.sub.blc.Circle;

public class CircleData {

	public static void main(String[] args) {
		double radius=Double.parseDouble(IO.readln("Enter the Radius : "));
		String CircleDataValidation=Circle.getAreaOfCircle(radius);
		double area=Double.parseDouble(CircleDataValidation);
		System.out.printf("Area of Circle is :%.2f",area);

	}

}
