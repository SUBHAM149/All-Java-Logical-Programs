// Find Car Properties
package com.sub.blc;

public class CarProperties {
    public String name;
    public int model;
    public String color;
    
    public void getCarInformation() {
    	System.out.println("My Name is :"+name);
    	System.out.println("My model is :"+model);
    	System.out.println("My color is :"+color);
    }
    public void drive() {
    	System.out.println("I always rady to go");
    }
    
}
package com.sub.elc;

import com.sub.blc.CarProperties;

public class CarAbout {

	public static void main(String[] args) {
		CarProperties bmw=new CarProperties();
         bmw.name="bmw";
         bmw.color="black";
         bmw.model=2025;
         bmw.getCarInformation();
         bmw.drive();
	}

}

